<link rel="apple-touch-icon" sizes="180x180" href="<%- config.root %>img/favicon/apple-icon-180x180.png">
<link rel="icon" type="image/png" sizes="192x192"  href="<%- config.root %>img/favicon/android-icon-192x192.png">
<link rel="icon" type="image/png" sizes="32x32" href="<%- config.root %>img/favicon/favicon-32x32.png">
<link rel="icon" type="image/png" sizes="96x96" href="<%- config.root %>img/favicon/favicon-96x96.png">
<link rel="icon" type="image/png" sizes="16x16" href="<%- config.root %>img/favicon/favicon-16x16.png">
<link rel="manifest" href="<%- config.root %>img/favicon/manifest.json">
<meta name="msapplication-TileColor" content="#ffffff">
<meta name="msapplication-TileImage" content="<%- config.root %>img/favicon/ms-icon-144x144.png">
<meta name="theme-color" content="#ffffff">

    <link rel="alternate" href="<% if (theme.rss){ %><%- theme.rss %><% } else { %><%- config.root %>atom.xml<% } %>" title="<%= config.title %>" type="application/atom+xml">
    <link rel="stylesheet" href="<%- config.root %>css/all.css" media="screen" type="text/css">
	<% if (theme.highlightjs) { %>
    <link rel="stylesheet" href="<%- config.root %>highlightjs/<%- theme.highlightjs %>.css" type="text/css">
    <% } %>


    <!-- Custom stylesheet, (add custom styles here, always load last) -->
    <!-- Load our stylesheet for IE8 -->
    <!--[if IE 8]>
    <link rel="stylesheet" type="text/css" href="<%- config.root %>css/ie8.css" />
    <![endif]-->

    <!-- Google Webfonts (Monserrat 400/700, Open Sans 400/600) -->

<%
  var title = page.title;

  if (is_archive()){
    title = 'Archives';

    if (is_month()){
      title += ': ' + page.year + '/' + page.month;
    } else if (is_year()){
      title += ': ' + page.year;
    }
  } else if (is_category()){
    title = 'Category: ' + page.category;
  } else if (is_tag()){
    title = 'Tag: ' + page.tag;
  }
%>
<head>
  <title><% if (title){ %><%= title %> | <% } %><%= config.title %></title>
  <!-- Meta data -->
    <meta http-equiv="Content-Type" content="text/html" charset="UTF-8" >
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="generator" content="<%- config.title %>">
    <meta name="author" content="<%- config.author %>">
    <meta name="description" content="<%- config.subtitle %>" />
    <meta name="keywords" content="<%- config.keywords %>" />

    <!-- Favicon, (keep icon in root folder) -->
    <!-- <link rel="Shortcut Icon" href="<%- config.root %>img/favicon.ico" type="image/ico"> -->

    <link rel="apple-touch-icon" sizes="57x57" href="<%- config.root %>img/favicon/apple-icon-57x57.png">
<link rel="apple-touch-icon" sizes="60x60" href="<%- config.root %>img/favicon/apple-icon-60x60.png">
<link rel="apple-touch-icon" sizes="72x72" href="<%- config.root %>img/favicon/apple-icon-72x72.png">
